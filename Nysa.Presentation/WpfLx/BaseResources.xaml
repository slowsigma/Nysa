               <Button Command="{Binding Path=WindowMinimizeCommand}" Visibility="{Binding Path=WindowMinimizeVisibility}" Style="{DynamicResource WindowMinimizeButton}"/>
               <Button Command="{Binding Path=WindowInfoCommand}" Visibility="{Binding Path=WindowInfoVisibility}" Style="{DynamicResource WindowInfoButton}"/>
            </StackPanel>
         </Grid>

         <AdornerDecorator Grid.Row="1">
            <ContentPresenter/>
         </AdornerDecorator>
      </Grid>
   </ControlTemplate>
   
